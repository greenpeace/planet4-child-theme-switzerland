{
	"env": {
		"browser": true,
		"es6": true,
		"jquery": true,
		"amd": true
	},
	"extends": ["plugin:@wordpress/eslint-plugin/recommended"],
	"globals": {
		"wp": "readonly"
	},
	"rules": {
		"linebreak-style": ["error", "unix"],
		"no-else-return": "off",
		"max-len": ["error", { "code": 160 }]
	}
}
